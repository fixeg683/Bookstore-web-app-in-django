{
  "builds": [
    {
      "src": "bookstore/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "test_project/wsgi.py"
    }
  ]
}
